{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": ["*.csproj"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/unity"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "markdownEngineName": "markdig",
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata":
    {
      "_appTitle": "UniOption Documentation",
      "_appFooter": "UniOption Documentation",
      "_enableSearch": true
    }
  }
}